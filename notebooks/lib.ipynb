    "def length_standard(n):\n",
    "    out = []\n",
    "    for i in range(n):\n",
    "        if i % 10 == 0:\n",
    "            out.append((i - 100*(i/100.)) / 10.)\n",
    "        elif i % 5 == 0:\n",
    "            out.append(\":\")\n",
    "        else:\n",
    "            out.append(\".\")\n",
    "    return \"\".join(map(str, out))"
   ]
